<form method="post" action="{{ path('lti_ags_delete', {'id': lti_ag.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ lti_ag.id) }}">
    <button class="btn">Delete</button>
</form>
