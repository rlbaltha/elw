{% extends 'base.html.twig' %}

{% block title %}Edit LtiAgs{% endblock %}

{% block subnav %}
    <a class="navbar-brand" href="{{ course_path('course_show', {'id': course.id}) }}">{{ course.name }}</a>
    {% if status !='Pending' %}
        <li class="nav-item">
            <a class="nav-link" href="{{ course_path('doc_index') }}">Documents</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" href="{{ course_path('journal_index') }}">Journal</a>
        </li>
    {% endif %}
{% endblock %}

{% block toolnav %}
    {{ include('course/_coursenav.html.twig') }}
{% endblock %}

{% block body %}
    <h1>Edit LtiAgs</h1>

    {{ include('lti_ags/_form.html.twig', {'button_label': 'Update'}) }}

    {{ include('lti_ags/_delete_form.html.twig') }}
{% endblock %}
