{% extends 'base.html.twig' %}

{% block title %}Ratingset index{% endblock %}

{% block body %}
    <h1>Ratingset index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for ratingset in ratingsets %}
            <tr>
                <td>{{ ratingset.id }}</td>
                <td>
                    <a href="{{ path('app_ratingset_show', {'id': ratingset.id}) }}">show</a>
                    <a href="{{ path('app_ratingset_edit', {'id': ratingset.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_ratingset_new') }}">Create new</a>
{% endblock %}
